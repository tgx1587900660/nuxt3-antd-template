{
  "name": "nuxt3-antd-template",
  "version": "1.0.0",
  "private": true,
  "description": "一个基于 nuxt3 和 Ant-Design-Vue 的模板项目",
  "license": "MIT",
  "author": "tangganxiao",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "nuxt build",
    "dev": "set NODE_OPTIONS=--max_old_space_size=8192 && nuxt dev --host -p 2999",
    "dev-https": "nuxt dev --https -p 4000",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "preview": "set PORT=3001 && nuxt preview",
    "sort": "npx sort-package-json"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@ant-design-vue/nuxt": "^1.4.6",
    "@ant-design/icons-vue": "^7.0.1",
    "@ckeditor/ckeditor5-alignment": "37.1.0",
    "@ckeditor/ckeditor5-autoformat": "37.1.0",
    "@ckeditor/ckeditor5-basic-styles": "37.1.0",
    "@ckeditor/ckeditor5-block-quote": "37.1.0",
    "@ckeditor/ckeditor5-core": "37.1.0",
    "@ckeditor/ckeditor5-editor-classic": "37.1.0",
    "@ckeditor/ckeditor5-essentials": "37.1.0",
    "@ckeditor/ckeditor5-font": "37.1.0",
    "@ckeditor/ckeditor5-heading": "37.1.0",
    "@ckeditor/ckeditor5-image": "37.1.0",
    "@ckeditor/ckeditor5-link": "37.1.0",
    "@ckeditor/ckeditor5-list": "37.1.0",
    "@ckeditor/ckeditor5-paragraph": "37.1.0",
    "@ckeditor/ckeditor5-theme-lark": "37.1.0",
    "@ckeditor/ckeditor5-utils": "37.1.0",
    "@nuxt/icon": "1.10.3",
    "@pinia/nuxt": "^0.5.3",
    "ant-design-vue": "^4.2.6",
    "colorthief": "^2.6.0",
    "docx-preview": "^0.3.2",
    "gsap": "^3.12.5",
    "lodash-es": "^4.17.21",
    "nodemailer": "^6.10.0",
    "nuxt": "^3.16.2",
    "pdfjs-dist": "^4.5.136",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@ckeditor/vite-plugin-ckeditor5": "^0.1.3",
    "@nuxt/devtools": "^1.7.0",
    "@types/lodash-es": "^4.17.12",
    "@types/nodemailer": "^6.4.17",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.7",
    "less": "^4.2.2",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.1",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sort-package-json": "^2.14.0",
    "tailwindcss": "^3.4.17"
  },
  "packageManager": "pnpm@9.15.0"
}
